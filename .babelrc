{
  "presets": [
    ["es2015", {
      "modules": false
    }],
    "stage-2",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    ["component",
      [
        {
          "libraryName": "bh-mint-ui2",
          "style": true
        }
      ]
    ],
    ["import", {
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }]
  ],
  "comments": false,
  "env": {
    "test": {
      "plugins": ["istanbul"]
    }
  }
}
